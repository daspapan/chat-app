{
  "name": "infra",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "git-clear": "git rm -r --cached .",
    "cdk:synth": "cdk synth --profile fsps-cdk-admin",
    "cdk:deploy": "npx aws-cdk deploy --require-approval never --all --outputs-file cdk-outputs.json --profile fsps-cdk-admin",
    "cdk:destroy": "npx aws-cdk destroy --require-approval never --all --force --profile fsps-cdk-admin"
  },
  "devDependencies": {
    "@types/git-branch": "^2.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "aws-cdk": "2.148.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.148.1-alpha.0",
    "aws-cdk-lib": "2.148.0",
    "constructs": "^10.0.0",
    "git-branch": "^2.0.1",
    "source-map-support": "^0.5.21"
  }
}
